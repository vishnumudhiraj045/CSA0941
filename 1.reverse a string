import java.util.*;
import java.io.*;
public class reverse{
  public static void main(String[] args){
    Scanner sc=new Scanner(System.in);
    String name=sc.nextInt();
    int len=name.length();
    String empty="";
    for(int i=len-1;i>=0;i--){
      empty=name.charAt(i);
      }
    System.out.print("Reversed string is:"+empty)
    }
  }

      
